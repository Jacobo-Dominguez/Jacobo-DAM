<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xs:element name="pescaderia">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="pescados"/>
        <xs:element ref="mariscos"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="pescados">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="pescado" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="pescado">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="nombre" type="xs:string"/>
        <xs:element name="tipo" type="xs:string"/>
        <xs:element name="temporada" type="xs:string"/>
        <xs:element name="procedencia" type="xs:string"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="mariscos">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="marisco" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="marisco">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="nombre" type="xs:string"/>
        <xs:element name="temporada" type="xs:string"/>
        <xs:element name="procedencia" type="xs:string"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>
